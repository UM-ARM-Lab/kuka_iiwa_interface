<launch>
    <arg name="use_left_arm" default="true"/>
    <arg name="use_right_arm" default="true"/>

    <group ns="victor">
        <param name="manual_motion/use_left_arm" exec="boolean" value="$(var use_left_arm)"/>
        <param name="manual_motion/use_right_arm" exec="boolean" value="$(var use_right_arm)"/>
        <node pkg="victor_hardware_interface" exec="manual_motion.py" name="manual_motion" output="screen"/>
    </group>

</launch>
