<launch>
    <arg name="use_left_arm" default="true"/>
    <arg name="use_right_arm" default="true"/>

    <node pkg="victor_hardware" exec="manual_motion.py" name="manual_motion" output="screen" namespace="victor">
        <param name="use_left_arm" value="$(var use_left_arm)"/>
        <param name="use_right_arm" value="$(var use_right_arm)"/>
    </node>

</launch>
